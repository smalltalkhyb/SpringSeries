<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">


    <bean id="beanPostProcessor" class="com.smalltalk.hyb.spring.CustomBeanPostProcessor">

    </bean>

    <bean id="instantiationAwareBeanPostProcessor" class="com.smalltalk.hyb.spring.CustomAwareBeanPostProcessor">
    </bean>

    <bean id="beanFactoryPostProcessor" class="com.smalltalk.hyb.spring.CustomBeanFactoryPostProcessor" >
    </bean>

    <bean id="orderInfo" class="com.smalltalk.hyb.spring.OrderInfo" init-method="orderInfoInit"
          destroy-method="orderInfoDestory"  p:name="周杰伦" p:price="100.00"
          p:id="1000"  scope="singleton"/>
</beans>
